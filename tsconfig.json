{
    "compilerOptions": {
        "outDir": "./out",
        "allowJs": true,
        "target": "es2017",
        "lib": [
            "es2017"
        ],
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noEmitOnError": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "resolveJsonModule": true
    },
    "include": [
        "./src/**/*"
    ]
}